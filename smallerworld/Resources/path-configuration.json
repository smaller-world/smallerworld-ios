{
  "settings": {},
  "rules": [
    {
      "patterns": ["^/spaces$", "^/world$", "^/universe$"],
      "properties": {
        "presentation": "replace_root"
      }
    },
    {
      "patterns": [
        "^/settings$",
        "/new$",
        "/edit$",
        "/add$",
        "/invite$",
        "^/login"
      ],
      "properties": {
        "context": "modal",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": ["^/scan_qr_code$"],
      "properties": {
        "view_controller": "qr_code_scanner",
        "context": "modal"
      }
    },
    {
      "patterns": ["^/login"],
      "properties": {
        "modal_dismiss_gesture_enabled": false
      }
    }
  ]
}
