{
  "settings": {},
  "rules": [
    {
      "patterns": ["^/spaces$", "^/world$", "^/universe$"],
      "properties": {
        "presentation": "replace_root"
      }
    },
    {
      "patterns": ["^/settings$", "/new$", "/edit$", "^/login"],
      "properties": {
        "context": "modal",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": ["^/login"],
      "properties": {
        "modal_dismiss_gesture_enabled": false
      }
    }
  ]
}
